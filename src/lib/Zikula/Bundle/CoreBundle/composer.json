{
    "name": "zikula/core-bundle",
    "type": "symfony-bundle",
    "description": "An bundle providing zikula-related symfony form extensions.",
    "keywords": [ "zikula" ],
    "homepage": "https://ziku.la",
    "license": "MIT",
    "authors": [
        {
            "name": "Zikula Team",
            "homepage": "https://ziku.la"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "composer/composer": "1.*",
        "composer/installers": "1.*",

        "zikula/legal-module": "3.*",
        "zikula/profile-module": "3.*",
        "zikula/oauth-module": "1.*",
        "zikula/pagelock-module": "1.*",
        "zikula/andreas08-theme": "3.*",
        "zikula/seabreeze-theme": "4.*",

        "symfony/symfony": "4.*",
        "symfony/monolog-bundle": "3.4.*",
        "sensio/framework-extra-bundle": "dev-master",
        "symfony/maker-bundle": "1.*",
        "elao/web-profiler-extra-bundle" : "2.*",
        "sensiolabs/security-checker" : "5.*",

        "jms/translation-bundle": "1.*",
        "nikic/php-parser": "4.*",
        "willdurand/js-translation-bundle": "2.*",

        "doctrine/orm": "2.*",
        "doctrine/doctrine-bundle": "2.*",
        "stof/doctrine-extensions-bundle": "1.*",
        "gedmo/doctrine-extensions": "2.*",

        "robloach/component-installer": "*",
        "michelf/php-markdown": "1.*",
        "twig/extensions": "1.*",
        "zikula/filesystem": "1.*",

        "oyejorge/less.php": "dev-master",
        "liip/imagine-bundle": "2.*",

        "zikula/generator-bundle": "3.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/zikula/Wizard"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zikula/GeneratorBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zikula-modules/Legal"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zikula-modules/Profile"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zikula-modules/PageLock"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zikula/OAuth"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zikula/ZikulaAndreas08Theme"
        },
        {
            "type": "vcs",
            "url": "https://github.com/zikula/ZikulaSeaBreezeTheme"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\Bundle\\CoreBundle\\": "" }
    }
}
