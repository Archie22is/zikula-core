{
    "name": "zikula/coreinstaller-bundle",
    "type": "symfony-bundle",
    "description": "An application to install or upgrade the Zikula Core.",
    "keywords": [ "zikula" ],
    "homepage": "https://ziku.la",
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Heydenburg",
            "email": "info@ziku.la"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "symfony/framework-bundle": "4.*",
        "symfony/console": "^4.3",
        "symfony/requirements-checker": "dev-patch-1 as 1.*",
        "matthiasnoback/symfony-console-form": "3.*",
        "zikula/wizard": "3.*"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/craigh/requirements-checker"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\Bundle\\CoreInstallerBundle\\": "" }
    }
}
